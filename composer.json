{
    "name": "jeremykendall/flaming-archer",
    "description": "A photo 365 project application. Requires user to have a Flickr API key",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremy Kendall",
            "homepage": "http://about.me/jeremykendall",
            "role": "Developer"
        }
    ],
    "keywords": ["photography", "flickr", "photo-a-day", "photo 365"],
    "repositories": [
    {
        "type": "composer",
        "url": "http://packages.zendframework.com/"
    }
    ],
    "require": {
        "php": ">=5.3.10",
        "ext-mcrypt": "*",
        "slim/slim": "2.1.0",
        "slim/extras": "2.0.1",
        "twig/twig": "1.10.3",
        "zendframework/zend-authentication": "2.0.*",
        "zendframework/zend-cache": "2.0.*",
        "zendframework/zend-serializer": "2.0.*",
        "rych/phpass": "2.1.x-dev"
    },
    "require-dev": {
        "composer/composer": "1.0.x-dev",
        "mikey179/vfsStream": "1.1.*"
    },
    "autoload": {
        "psr-0": 
        {
            "Fa": "library/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Fa\\Composer\\Script\\Config::create",
            "Fa\\Composer\\Script\\SQLite::prepare"
        ],
        "post-update-cmd": [
            "Fa\\Composer\\Script\\Config::create",
            "Fa\\Composer\\Script\\SQLite::prepare"
        ]
    }
}
